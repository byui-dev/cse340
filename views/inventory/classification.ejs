<% if (title) { %>
    <h1><%= title %></h1>
<% } %>

<% if (messages && messages.length > 0) { %>
 <ul class="notice">
   <% messages.forEach(function(msg) { %>
    <li><%= msg %></li>
   <% }) %>
  </ul>
<% } %>

<% if (errors) { %>
  <ul class="notice">
    <% errors.forEach(function(error) { %>
     <li><%= error.msg %></li>
    <% }) %>
    </ul>   
<% } %>

<%- grid %>
